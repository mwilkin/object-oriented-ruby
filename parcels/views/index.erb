<h1>Welcome to Yasskin Global Shipping</h1>

<form action="/results">
  <div class="form-group">
    <label for="height">Height: </label>
    <input name="height" id="height" class="form-control" type="number">
  </div>
  <div class="form-group">
    <label for="length">Length: </label>
    <input name="length" id="length" class="form-control" type="number">
  </div>
  <div class="form-group">
    <label for="width">Width: </label>
    <input name="width" id="width" class="form-control" type="number">
  </div>
  <div class="form-group">
    <label for="weight">Weight (in pounds): </label>
    <input name="weight" id="weight" class="form-control" type="number">
  </div>
  <button type="submit" class="btn-success">Send</button>
</form>
